    "test:tdd": "cross-env NODE_ENV=test jest --watch",
    "release": "HUSKY=0 standard-version",
    "pub:n": "HUSKY=0 bumpp --commit 'chore: v%s' --push && npm publish --tag next"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,vue}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
