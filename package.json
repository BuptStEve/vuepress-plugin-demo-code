{
    "name": "vuepress-plugin-demo-code",
    "version": "0.6.0",
    "description": "üìù Demo and code plugin for vuepress",
    "main": "src/index.js",
    "files": [
        "src"
    ],
    "scripts": {
        "prepare": "husky install",
        "cov": "open coverage/lcov-report/index.html",
        "docs": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "lint": "eslint --fix . docs/.vuepress/ --ext .js,.vue --ignore-path .gitignore",
        "test": "cross-env NODE_ENV=test jest",
        "test:tdd": "cross-env NODE_ENV=test jest --watch",
        "release": "standard-version",
        "next:pm": "npm --no-git-tag-version version preminor",
        "next:pr": "npm --no-git-tag-version version prerelease",
        "pub": "npm publish",
        "pub:n": "npm publish --tag next"
    },
    "lint-staged": {
        "{src,test}/**/*.{js,vue}": [
            "eslint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "codesandbox-import-utils": "^2.2.3",
        "markdown-it-container": "^3.0.0",
        "prismjs": "^1.23.0",
        "vuepress-plugin-code-copy": "^1.0.6"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.7",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@vue/test-utils": "^1.2.1",
        "all-contributors-cli": "^6.20.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "codecov": "^3.8.2",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.29.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-config-vue": "^2.0.2",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^7.11.1",
        "gh-pages": "^3.2.3",
        "husky": "^6.0.0",
        "jest": "^27.0.4",
        "jest-serializer-vue": "^2.0.2",
        "jest-transform-stub": "^2.0.0",
        "lint-staged": "^11.0.0",
        "markdown-it-include": "^2.0.0",
        "rimraf": "^3.0.2",
        "standard-version": "^9.3.0",
        "vue-jest": "^3.0.7",
        "vuepress": "^1.8.2",
        "vuepress-plugin-smooth-scroll": "^0.0.10"
    },
    "keywords": [
        "vue",
        "code",
        "demo",
        "vuepress",
        "demo-code",
        "documentation"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BuptStEve/vuepress-plugin-demo-code.git"
    },
    "homepage": "https://buptsteve.github.io/vuepress-plugin-demo-code/",
    "author": "StEve Young",
    "license": "MIT"
}
